{
    "manifest_version": 3,
    "name": "Indra's ad spam",
    "version": "1.1",
    "description": "Let the cute ads take over your screen~",
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }