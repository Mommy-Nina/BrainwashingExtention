{
    "manifest_version": 3,
    "name": "Corruptor",
    "version": "1.0",
    "description": "Replaces all images on a webpage with silly images ;)",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }